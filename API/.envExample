# Port de l'application
PORT=5000
# URI de connexion à MongoDB 
MONGO_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority
# Secret pour JWT 
JWT_SECRET=<votre_jwt_secret>
# Clé API Hunter 
HUNTER_API_KEY=<votre_hunter_api_key>
# Configuration email pour envoi d'e-mails
EMAIL_USER=<votre_email>
EMAIL_PASSWORD=<votre_mot_de_passe_email>
# Clé API SecurityTrails
SECURITYTRAILS_API_KEY=<votre_securitytrails_api_key>
# Clé API SerpAPI
SERPAPI_API_KEY=<votre_serpapi_api_key>
